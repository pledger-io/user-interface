{
  "name": "pledger-ui",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://dev.ota.pledger.io",
  "dependencies": {
    "@mdi/js": "^7.2.96",
    "@mdi/react": "^1.6.1",
    "axios": "^1.3.5",
    "chart.js": "^4.4.0",
    "chartjs-adapter-spacetime": "^1.0.1",
    "spacetime": "^7.4.7",
    "cra-template": "1.2.0",
    "jwt-decode": "^3.1.2",
    "react": "^18.2.0",
    "react-datepicker": "^4.18.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.46.2",
    "react-router-dom": "^6.16.0",
    "react-scripts": "5.0.1",
    "sass": "^1.68.0",
    "web-vitals": "^3.4.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "prop-types": "^15.8.1",
    "tailwindcss": "^3.3.3",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "jest": {
    "testMatch": [
      "**/?(*.)(spec|test).js?(x)"
    ],
    "coverageReporters": [
      "json"
    ],
    "moduleNameMapper": {
      "axios": "<rootDir>/__mocks__/axios.js",
      "spacetime": "spacetime/builds/spacetime.cjs"
    }
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "no-unused-vars": [
        "warn",
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_",
          "caughtErrorsIgnorePattern": "^_"
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
